logoutput: /dev/null
internal: {{ ansible_default_ipv4.interface }} port = {{ SOCKS_PORT }}
external: {{ ansible_default_ipv4.address }}
socksmethod: none
clientmethod: none
user.privileged: proxy
user.notprivileged: nobody
client pass {
    from: 0.0.0.0/0 to: 0.0.0.0/0
}
socks pass {
    from: 0.0.0.0/0 to: 0.0.0.0/0
    protocol: tcp udp
}
